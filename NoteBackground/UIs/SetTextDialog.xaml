<Window x:Class="NoteBackground.UIs.SetTextDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NoteBackground.UIs"
        mc:Ignorable="d"
        Title="设置文本" Height="150" Width="400">
    <StackPanel>
        <TextBox Name="TbText"></TextBox>
        <Button IsCancel="True" IsDefault="True" Click="OkClick">确定</Button>
        <Button IsCancel="True">取消</Button>
    </StackPanel>
</Window>
